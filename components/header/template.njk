   <button type="button" class="bc-header__nav-toggle" id="burger">
      Menu 
      <span class="bc-header__nav-toggle-icon">
         {{ bcIcon({
            variant: 'bars'
         }) }}
      </span>
   </button>

   <div class="bc-header__main-menu-container" id="navbar">
      <div class="bc-header__row">
         <div class="bc-header__col">
            <button class="bc-header__nav-share-toggle" id="open-flyout">Share</button>
            <div class="bc-header__share-flyout" id="flyout">
               <button class="bc-header__share-flyout-close" id="close-flyout">
                  {{ bcIcon({
                     variant: 'xmark'
                  }) }}
                  <span class="sr-only">Close</span>
               </button>
               <div class="bc-header__social-icons-container">
                  <a class="bc-header__social-icon">
                     <i class="fab fa-twitter"></i>
                     <span class="sr-only">Twitter</span>
                  </a>
                  <a class="bc-header__social-icon">
                     <i class="fab fa-linkedin-in"></i>
                     <span class="sr-only">LinkedIn</span>
                  </a>
                  <a class="bc-header__social-icon">
                     <i class="fab fa-facebook-f"></i>
                     <span class="sr-only">Facebook</span>
                  </a>
                  <a class="bc-header__social-icon">
                     <i class="fab fa-whatsapp"></i>
                     <span class="sr-only">WhatsApp</span>
                  </a>
                  <a class="bc-header__social-icon">
                     <i class="fas fa-envelope"></i>
                     <span class="sr-only">Email</span>
                  </a>
               </div>
            </div>
         </div>
         <div class="bc-header__col">
            <button class="bc-header__nav-close" id="close">Close</button>
         </div>
      </div>
		<ul class="bc-header__main-menu">
         <li class="bc-header__menu-item menu-item--blue">
            <a href="#" title="">
               Home
            </a>
         </li>
         <li class="bc-header__menu-item bc-header__menu-item-has-children menu-item--teal" data-toggle="bc-header__menu-item-sub-menu">
            <a href="#" title="">
               About us
               <span class="bc-header__menu-item-toggle">
                  {{ bcIcon({
                     variant: 'plus'
                  }) }}
               </span>
            </a>
            <ul class="bc-header__menu-item-sub-menu">
               <li class="bc-header__menu-item">
                  <a href="#" title="">Publications and resources</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Work with UK Youth</a>
               </li>
            </ul>
         </li>
         <li class="bc-header__menu-item bc-header__menu-item-has-children menu-item--indigo" data-toggle="bc-header__menu-item-sub-menu">
            <a href="#" title="">
               What we do
               <span class="bc-header__menu-item-toggle">
                  {{ bcIcon({
                     variant: 'plus'
                  }) }}
               </span>
            </a>
            <ul class="bc-header__menu-item-sub-menu">
               <li class="bc-header__menu-item">
                  <a href="#" title="">Programmes</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Avon Tyrrell</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">The UK Youth Fund: Thriving Minds</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">UK Youth Movement</a>
                  </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Quality marks</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Are You Convinced? Podcast</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Resources and reports</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Digital Resource Hub</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">EmpowHER Toolkit</a>
               </li>
            </ul>
         </li>
         <li class="bc-header__menu-item bc-header__menu-item-has-children menu-item--orange" data-toggle="bc-header__menu-item-sub-menu">
            <a href="#" title="">
               Get involved
               <span class="bc-header__menu-item-toggle">
                  {{ bcIcon({
                     variant: 'plus'
                  }) }}
               </span>
            </a>
            <ul class="bc-header__menu-item-sub-menu">
               <li class="bc-header__menu-item">
                  <a href="#" title="">Donate</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Join the UK Youth Movement</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Partner with us</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Fundraise with us</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Read More March</a>
               </li>
               <li class="bc-header__menu-item">
                  <a href="#" title="">Stay in touch</a>
               </li>
            </ul>
         </li>
         <li class="bc-header__menu-item menu-item--red">
            <a href="#" title="">Events</a>
         </li>
         <li class="bc-header__menu-item menu-item--pink">
            <a href="#" title="">News and blogs</a>
         </li>
         <li class="bc-header__menu-item menu-item--blue">
            <a href="#" title="">Contact us</a>
         </li>
         <li class="bc-header__menu-item menu-item--blue">
            <a href="#" title="">Donate</a>
         </li>
         <li class="bc-header__menu-item menu-item--red">
            <a class="bc-header__navbar-search">
               Search
               {{ bcIcon({
                  variant: 'magnifying-glass'
               }) }}
            </a>
         </li>
         {# <li class="fs-!-margin-top-2 fs-!-margin-bottom-1">
            <button class="bc-header__navbar-account">
               {{ bcIcon({
                  variant: 'user'
               }) }}
               My account
            </button>
         </li> #}
         {# <li>
            <button class="bc-header__navbar-user">
               {{ bcIcon({
                  variant: 'arrow-right-from-bracket'
               }) }}
               Log out
            </button>
         </li> #}
         <li>
           <button class="bc-header__navbar-user">
               {{ bcIcon({
                  variant: 'user'
               }) }}
               Log in /Register
            </button>
         </li>
      </ul>
   </div>

   {{ bcButton({
      classes: 'bc-header__navbar-donate',
      appearance: 'secondary',
      text: 'Donate',
      href: '#'
   }) }}

   <button class="bc-header__navbar-search bc-button-primary">
      Search
      {{ bcIcon({
         variant: 'magnifying-glass'
      }) }}
   </button>

   {# <button class="bc-header__navbar-account">
      {{ bcIcon({
         variant: 'user'
      }) }}
